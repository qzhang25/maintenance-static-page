{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*"
    ]
  },
  "globalHeaders": {
    "Cache-Control": "no-store, no-cache, must-revalidate, max-age=0",
    "Pragma": "no-cache",
    "Expires": "0"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}